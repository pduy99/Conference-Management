                  <ImageView fx:id="cellLogo" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
               </children></VBox>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="118.0" prefWidth="380.0" spacing="5.0">
               <children>
                  <Label fx:id="tfConferenceName" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="368.0" style="-fx-text-fill: #B00805; -fx-font-size: 20px; -fx-font-weight: bold;" text="Label" textFill="#000022" wrapText="true">
                     <font>
                        <Font name="Baghdad Regular" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Label>
                  <Text fx:id="tfShortDescription" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #B00805; -fx-font-size: 18px;" text="Short Description here" wrappingWidth="350.0" />
