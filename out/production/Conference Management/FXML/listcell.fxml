                  <JFXButton fx:id="btnEnroll" alignment="CENTER" prefHeight="25.0" prefWidth="130.0" style="-fx-cursor: hand; -fx-background-color: #B00805;" styleClass="listButtons" stylesheets="@../css/fullpackstyling.css" text="Enroll me" textFill="#f2f7f9">
